//打印左下角三角和
#include<stdio.h>
int main()
{
int n, a[10][10], s=0, i, j;
    printf("### 输入样例:\n");
    scanf("%d", &n);
        for (i = 0;i < n;i++) {
        for (j = 0;j < n;j++) {
            scanf("%d", &a[i][j]);
        }
    }
    for (i = 0;i < n;i++) {
        for (j = 0;j < i + 1;j++) {
            s += a[i][j];
        }
    }
    printf("### 输出样例:\n%d", s);
    return 0;

}
